#include <iostream>
#include <string>
#include <stack>
using namespace std;

int OperationPriority (string s)
{
  switch (s[0])
  {
    case '(': return 1;
    case '-': return 2;
    case '+': return 2;
    case '/': return 3;
    case '*': return 3;
  }
  defaut:
  {
    cerr << "Error";
    return 0;
  }
}

bool IsOperation(const string s)
{
  if (s.length() > 1)
    return 0;
  switch (s[0])
  {
    case '+':
    case '-':
    case '*':
    case '/':
    return true;
  default:
    return false;
  }
}

string RPN_converter(string expression)
{
  expression += " ";
  string expression_out{};
  stack <string> rpn_macine{};
  string temp_str{};

  for (int i = 0; i < expression.length(); ++i)
  {

    if (expression[i] != ' ')
    {
      temp_str.push_back(expression[i]);
    }

    else if (!IsOperation(temp_str))
    {
      expression_out += temp_str;
      temp_str = "";
    }

    else if (IsOperation(temp_str))
    {
      if ((rpn_macine.empty()) && (OperationPriority(rpn_macine.top()) < OperationPriority(temp_str)))
      {
        rpn_macine.push(temp_str);
      }

      if (OperationPriority(rpn_macine.top()) >= OperationPriority(temp_str))
      {
        while (OperationPriority(rpn_macine.top()) >= OperationPriority(temp_str))
        {
          expression_out += rpn_macine.top();
          rpn_macine.pop();
        }
        rpn_macine.push(temp_str);
      }
    }

    else if (temp_str == "(")
    {
      rpn_macine.push(temp_str);
    }

    else if (temp_str == ")")
    {
      while (rpn_macine.top() != "(")
      {
        expression_out += rpn_macine.top();
        rpn_macine.pop();
      }
      rpn_macine.pop();
    }

  temp_str = "";
  }

  if (!rpn_macine.empty())
  {
    expression_out += rpn_macine.top();
    rpn_macine.pop();
  }

  return expression_out;
}

int main()
{
  string s;
  getline(cin, s);
  string res = RPN_converter(s);
  cout << res;
  return 0;
}
